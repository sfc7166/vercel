{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/products",        
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",        
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",        
        "dest": "build/index.html"
      },
      {
        "src": "/(.+)",        
        "dest": "build/$1"
      }
    ]
  }